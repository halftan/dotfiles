
xterm*termName: xterm-256color
xterm*locale: true
xterm*altIsNotMeta: true
xterm*altSendsEscape: true
xterm*saveLines: 4096

xterm*faceName: dejavu sans mono for powerline:size=10:style:book:antialias=true
xterm*faceNameDoublesize: WenQuanYi Micro Hei:size=10:style:regular:antialias=false
xterm*renderFont: true
xterm*faceSize: 10

URxvt.font: \
        xft:Source Code Pro for Powerline:Regular:size=10, \
        xft:Inconsolata for Powerline:Medium:size=10, \
        xft:dejavu sans mono for powerline:book:size=10, \
        xft:WenQuanYi Micro Hei:regular:size=10

URxvt*loginShell: true

! Compile xft: Turn on/off double-buffering for xft
URxvt*buffered: true

! Set scrollbar style to rxvt, plain, next or xterm.
URxvt*scrollstyle: rxvt

! Specifies the terminal type name to be set in the
URxvt*termName: xterm-256color

URxvt.perl-ext-common:          default,tabbedex,clipboard,url-select,keyboard-select
URxvt.url-select.launcher:      chromium
URxvt.url-select.underline:     true
URxvt.keysym.M-u:               perl:url-select:select_next
URxvt.keysym.M-Escape:          perl:keyboard-select:activate
URxvt.keysym.M-s:               perl:keyboard-select:search
URxvt.tabbed.autohide:          true
URxvt.keysym.Control-t:         perl:tabbedex:new_tab
URxvt.keysym.Control-Tab:       perl:tabbedex:next_tab
URxvt.keysym.Control-Shift-Tab: perl:tabbedex:prev_tab

